{
  "name": "serviceforce-develop-tool",
  "version": "1.0.5",
  "main": "dist/electron/main/index.js",
  "description": "ServiceForce Develop Tool",
  "author": "tjyuanepng@gmail.com",
  "private": true,
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite",
    "build": "npx rimraf release && vue-tsc --noEmit && vite build && electron-builder",
    "upload": "node --experimental-json-modules ./scripts/upload.mjs",
    "docs:dev": "vitepress dev docs",
    "docs:build": "npx rimraf docs/node_modules && vitepress build docs",
    "docs:serve": "vitepress serve docs"
  },
  "engines": {
    "node": ">=14"
  },
  "devDependencies": {
    "@types/http-proxy": "^1.17.9",
    "@types/semver": "^7.3.12",
    "@vitejs/plugin-vue": "^2.3.3",
    "axios": "^0.27.2",
    "electron": "^19.0.8",
    "electron-builder": "^23.1.0",
    "electron-log": "^4.4.8",
    "electron-store": "^8.0.2",
    "element-plus": "^2.2.12",
    "form-data": "^4.0.0",
    "hotkeys-js": "^3.9.4",
    "http-proxy": "^1.18.1",
    "just-diff": "^5.1.1",
    "lodash-es": "^4.17.21",
    "pinia": "^2.0.18",
    "prettier": "^2.7.1",
    "semver": "^7.3.7",
    "typescript": "^4.7.4",
    "user-agent-data-types": "^0.3.0",
    "vite": "^3.0.9",
    "vite-plugin-electron": "^0.8.8",
    "vite-plugin-vue-element-plus-icon": "^1.0.5",
    "vitepress": "1.0.0-alpha.9",
    "vue": "^3.2.37",
    "vue-router": "^4.1.3",
    "vue-tsc": "^0.39.4"
  },
  "env": {
    "VITE_DEV_SERVER_HOST": "127.0.0.1",
    "VITE_DEV_SERVER_PORT": 3344
  },
  "keywords": [
    "electron",
    "vite",
    "vue3",
    "vue"
  ]
}
